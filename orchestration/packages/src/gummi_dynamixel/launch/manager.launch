<!-- -*- mode: XML -*- -->

<launch>
    <node name="dynamixel_manager_arm" pkg="dynamixel_controllers" type="controller_manager.py" required="true" output="screen">
        <rosparam>
            namespace: dxl_manager
            serial_ports:
                gummi_port_arm:
                    <!-- Equivalent to /dev/ttyUSB1 can be find at /dev/serial/by-id -->
                    port_name: "/dev/serial/by-id/usb-FTDI_FT232R_USB_UART_AI0284F3-if00-port0"
                    baud_rate: 1000000
                    min_motor_id: 10
                    max_motor_id: 33
                    update_rate: 60
                gummi_port_body:
                    <!-- Equivalent to /dev/ttyUSB0 can be find at /dev/serial/by-id -->
                    port_name: "/dev/serial/by-id/usb-FTDI_FT232R_USB_UART_AL02L1E5-if00-port0"
                    baud_rate: 1000000
                    min_motor_id: 1
                    max_motor_id: 101
                    update_rate: 60
        </rosparam>
    </node>
</launch>
